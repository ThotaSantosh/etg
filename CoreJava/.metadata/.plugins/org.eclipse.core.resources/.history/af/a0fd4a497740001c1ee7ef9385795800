package com.sonata.Employee;

public abstract class EmployeeExample {
abstract void calculateSalary();
public void display() {}

public class TechnicalEmployee extends EmployeeExample{
	private int empId;
	private String empName;
	private String address;
	private double basicPay;	
}
public class Staff extends EmployeeExample{
	private int empId;
	private String empName;
	private String address;
	private double basicPay;

		
	}
	
}

